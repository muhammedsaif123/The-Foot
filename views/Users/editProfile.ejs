<script>
   
    let name=document.getElementById("name")
    let mobile=document.getElementById("mobile")
    let city=document.getElementById("city")
    let pincode=document.getElementById("pincode")
    let address=document.getElementById("address")
 

    name.addEventListener("input",()=>{
      if(name.value.trim()==''){
        name.setCustomValidity("name cannot be null")
      }else if(name.value.trim().length<4){
        name.setCustomValidity("minimum 4 letters required")
      }
      else{
        name.setCustomValidity("")
      }
    })
    mobile.addEventListener("input",()=>{
      if (mobile.value.trim()==""){
        mobile.setCustomValidity("mobile cannot be null")

      }else{
        mobile.setCustomValidity("")
      }
    })
    city.addEventListener("input",()=>{
      if(city.value.trim()==""){
        city.setCustomValidity("city cannot be null")
      }else{
        city.setCustomValidity("")
      }
    })
    pincode.addEventListener("input",()=>{
      if(pincode.value.trim()==""){
        pincode.setCustomValidity("pincode field cannot be empty")
      }else{
        pincode.setCustomValidity("")
      }
    })
    address.addEventListener("input",()=>{
      if(address.value.trim()==""){
        address.setCustomValidity("address field cannot be empty")
      }else{
        address.setCustomValidity("")
      }
    })
    const form = document.getElementById("myForm")
    form.addEventListener("submit",(event)=>{
      event.preventDefault();
      alert("Add Product")
      form.submit()
    })
   



</script>